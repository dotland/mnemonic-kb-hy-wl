<dead_stroke> <Armenian_ben>                    : "∉"   U2209 # NOT AN ELEMENT OF
<dead_stroke> <Armenian_BEN>                    : "∉"   U2209 # NOT AN ELEMENT OF
<dead_stroke> <Armenian_vo>                     : "∩"   upshoe # INTERSECTION
<dead_stroke> <Armenian_VO>                     : "∩"   upshoe # INTERSECTION
