<dead_stroke> <Armenian_VEV>                    : "∧"   upcaret # LOGICAL AND
<dead_stroke> <Armenian_nu>                     : "¬"   notsign # NOT SIGN
<dead_stroke> <Armenian_NU>                     : "¬"   notsign # NOT SIGN
