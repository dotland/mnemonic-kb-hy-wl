# UTF-8 (Unicode) compose sequences
#
# Modified for Armenian (mnemonic) by [Dotland](https://github.com/dotland)
#
# Make sure GTK_IM_MODULE="xim" is set to support multichar input.
#
# Use the sequences from en_US.UTF-8 as the basis:
# include "/usr/share/X11/locale/en_US.UTF-8/Compose"

# Armenian letters and marks accessed by dead_acute
<dead_acute> <space>                            : "'"   apostrophe # APOSTROPHE
<dead_acute> <dead_acute>                       : "''"
<dead_acute> <Armenian_yech>                    : "է"   Armenian_e # ARMENIAN SMALL LETTER EH
<dead_acute> <Armenian_YECH>                    : "Է"   Armenian_E # ARMENIAN CAPITAL LETTER EH
<dead_acute> <Armenian_je>                      : "ճ"   Armenian_tche # ARMENIAN SMALL LETTER CHEH
<dead_acute> <Armenian_JE>                      : "Ճ"   Armenian_TCHE # ARMENIAN CAPITAL LETTER CHEH
<dead_acute> <Armenian_pyur>                    : "պ"   Armenian_pe # ARMENIAN SMALL LETTER PEH
<dead_acute> <Armenian_PYUR>                    : "Պ"   Armenian_PE # ARMENIAN CAPITAL LETTER PEH
<dead_acute> <Armenian_re>                      : "ռ"   Armenian_ra # ARMENIAN SMALL LETTER RA
<dead_acute> <Armenian_RE>                      : "Ռ"   Armenian_RA # ARMENIAN CAPITAL LETTER RA
<dead_acute> <dead_belowtilde>                  : "տ"   Armenian_tyun # ARMENIAN SMALL LETTER TIWN
<dead_acute> <dead_tilde>                       : "Տ"   Armenian_TYUN # ARMENIAN CAPITAL LETTER TIWN
<dead_acute> <dead_abovecomma>                  : "ծ"   Armenian_tsa # ARMENIAN SMALL LETTER CA
<dead_acute> <dead_abovereversedcomma>          : "Ծ"   Armenian_TSA # ARMENIAN CAPITAL LETTER CA
<dead_acute> <Armenian_vyun>                    : "և"   Armenian_ligature_ew # ARMENIAN SMALL LIGATURE ECH YIWN
<dead_acute> <Armenian_VYUN>                    : "և"   Armenian_ligature_ew # ARMENIAN SMALL LIGATURE ECH YIWN
<dead_acute> <U002D>                            : "֊"   Armenian_yentamna # ARMENIAN HYPHEN
<dead_acute> <comma>                            : "՚"   Armenian_apostrophe # ARMENIAN APOSTROPHE
<dead_acute> <period>                           : "·"   periodcentered # MIDDLE DOT
<dead_acute> <less>                             : "‹"   U2039 # SINGLE LEFT-POINTING ANGLE QUOTATION MARK
<dead_acute> <greater>                          : "›"   U203A # SINGLE RIGHT-POINTING ANGLE QUOTATION MARK
<dead_acute> <dead_stroke>                      : "՛"   Armenian_shesht # ARMENIAN EMPHASIS MARK
<dead_acute> <Armenian_o>                       : "°"   degree # DEGREE SIGN
<dead_acute> <Armenian_O>                       : "°"   degree # DEGREE SIGN
<dead_acute> <bracketleft>                      : "‘"   leftsinglequotemark # LEFT SINGLE QUOTATION MARK
<dead_acute> <bracketright>                     : "’"   rightsinglequotemark # RIGHT SINGLE QUOTATION MARK

